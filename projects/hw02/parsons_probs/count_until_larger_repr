def count_until_larger(num): #0given
!BLANK = num % 10  #1given #blankrightmost
!BLANK = 0  #1given #blankdistance
while !BLANK:  #1given #blanknum > 0
if num % 10 > !BLANK:  #2given #blankrightmost
return !BLANK  #3given #blankdistance
!BLANK //= !BLANK  #2given #blanknum #blank10
!BLANK += !BLANK  #2given #blankdistance #blank1
return !BLANK  #1given #blank-1
print('DEBUG:', !BLANK)
print('DEBUG:', !BLANK)
